<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="601.0" spacing="50.0">
         <children>
            <VBox>
               <children>
                  <Label layoutX="30.0" layoutY="30.0" text="Name:" />
                  <TextField fx:id="nameTextField" prefHeight="39.0" prefWidth="485.0" HBox.hgrow="ALWAYS" />
               </children>
            </VBox>
            <VBox layoutX="30.0" layoutY="30.0">
               <children>
                  <Label layoutX="30.0" layoutY="30.0" text="Model:" />
                  <TextField fx:id="modelTextField" prefHeight="39.0" prefWidth="485.0" HBox.hgrow="ALWAYS" />
               </children>
            </VBox>
            <HBox alignment="CENTER">
               <children>
                  <Button fx:id="submitButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" style="-fx-alignment: center;" text="Submit" textAlignment="CENTER">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
